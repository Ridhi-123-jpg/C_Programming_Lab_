#include <stdio.h>
int main()
{
    int n;
    printf("enterv the number");
    scanf("%d", &n);
    int c = 0, i = 2;
    while (i <= n / 2)
    {
        if (n % i == 0)
        {
            c = 1;
            break;
        }
        i++;
    }
    if (n == 1)
    {
        printf("1 is niether prime nor composite");
    }
    else if (c == 0)
    {
        printf("%d is a prime number", n);
    }
    else
    {
        printf("%d is a composite number");
    }
}
